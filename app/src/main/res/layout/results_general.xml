<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/mainpicture1">

    <TextView
        android:id="@+id/button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="31dp"
        android:gravity="center"
        android:lineSpacingExtra="30sp"
        android:text="@string/RESULTS_TABLE"
        android:textAllCaps="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textStyle="bold"
        android:typeface="sans" />


    <fragment
        android:id="@+id/fr1"
        android:name="com.example.galv.myapplication.fragmentOneList"
        android:layout_width="match_parent"
        android:layout_marginTop="31dp"
        android:layout_height="191dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="50dp"
        android:layout_weight="10"></fragment>
    <fragment
        android:id="@+id/fr2"
        android:layout_width="match_parent"
        android:layout_height="191dp"

        android:name="com.example.galv.myapplication.activitys.fragmentTwoMap"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="20dp"
        android:layout_weight="10"></fragment>

